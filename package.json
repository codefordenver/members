{
  "name": "members",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10.8.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.4.0",
    "@material-ui/icons": "^3.0.1",
    "@types/graphql": "14.0.1",
    "@types/jest": "^23.3.9",
    "@types/marked": "^0.4.2",
    "@types/node": "^10.12.2",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-select": "^2.0.8",
    "apollo-cache-inmemory": "^1.3.8",
    "apollo-client": "^2.4.5",
    "apollo-link": "^1.2.3",
    "apollo-link-context": "^1.0.9",
    "apollo-link-http": "^1.5.5",
    "apollo-link-rest": "^0.2.4",
    "auth0-js": "^9.8.1",
    "babel-polyfill": "^6.26.0",
    "date-fns": "^1.29.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "marked": "^0.5.1",
    "react": "^16.6.0",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.6.0",
    "react-ga": "^2.5.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.1",
    "react-select": "^2.1.1",
    "react-testing-library": "^5.2.3",
    "whatwg-fetch": "^3.0.0"
  },
  "lint-staged": {
    "{src,bin,server/src}/**/*.{js,json,css,ts,tsx,graphql,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "concurrently \"gql-gen\" \"react-scripts start\"",
    "start:no-gql-gen": "react-scripts start",
    "build": "react-scripts build && cpy build/index.html build --rename 404.html",
    "lint": "eslint . --ext .js --ext .graphql",
    "postinstall": "cd ./server && npm install",
    "preinstall": "npm run check-version",
    "setup": "node ./bin/auth0setup/promptUser.js",
    "gc": "cd ./server && dotenv -e ../.env.local graphcool",
    "_forceDeploy": "cd ./server && graphcool deploy --force",
    "gc:forceDeploy": "dotenv -e .env.local npm run _forceDeploy",
    "deployProdBackend": "cd ./server && graphcool deploy",
    "check-version": "node ./bin/checkVersion.js",
    "download-schema": "dotenv -e ./.env.local node ./bin/download-schema.js",
    "typecheck": "tsc --noEmit",
    "gql-gen": "gql-gen"
  },
  "homepage": "https://members.codefordenver.org",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cpy-cli": "^2.0.0",
    "dotenv": "^6.1.0",
    "dotenv-cli": "^1.4.0",
    "eslint": "5.6.0",
    "eslint-plugin-graphql": "^2.1.1",
    "graphcool": "^0.9.2",
    "graphql-code-generator": "^0.14.5",
    "graphql-codegen-typescript-client": "^0.14.5",
    "graphql-codegen-typescript-react-apollo": "^0.14.5",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "typescript": "^3.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
